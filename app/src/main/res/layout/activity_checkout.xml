<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <data/>

    <FrameLayout
        android:id="@+id/frameActivitychecko"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        style="@style/groupStylewhite_A700">

        <LinearLayout
            android:id="@+id/linearColumngroupfive"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_20pxh"
            android:layout_marginStart="@dimen/_20pxh"
            android:layout_marginTop="@dimen/_25pxv"
            android:layout_width="match_parent"
            android:orientation="vertical">

            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_width="match_parent">

                <LinearLayout android:gravity="start"
                    android:id="@+id/linearRowgroupfive"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:orientation="horizontal">

                    <ImageView android:id="@+id/imageGroupFive"
                        android:layout_height="@dimen/_14pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="@dimen/_14pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/back_arrow"
                        tools:ignore="ContentDescription"/>

                    <TextView android:ellipsize="end"
                        android:id="@+id/txtCheckout"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_21pxh"
                        android:layout_weight="0.84"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_checkout"
                        style="@style/txtOverpassbold16_1"/>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>

            <LinearLayout
                android:id="@+id/linearRow2itemsinyour"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_34pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/txt2Itemsinyour"
                    style="@style/txtOverpassromanregular14"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_marginBottom="@dimen/_15pxv"
                    android:layout_weight="0.67"
                    android:ellipsize="end"
                    android:textSize="15sp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:text="@string/msg_2_items_in_your"
                    tools:ignore="TextContrastCheck,TextContrastCheck" />

                <LinearLayout
                    android:gravity="end"
                    android:id="@+id/linearColumntotal"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_144pxh"
                    android:layout_weight="0.33"
                    android:layout_width="0dp"
                    android:orientation="vertical">

                    <TextView
                        android:id="@+id/txtTOTAL"
                        style="@style/txtOverpassromanregular13"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_18pxh"
                        android:layout_marginEnd="@dimen/_4pxh"
                        android:ellipsize="end"
                        android:singleLine="true"
                        android:text="@string/lbl_total2"
                        android:textSize="15sp"
                        android:textColor="#1976D2"
                        tools:ignore="TextContrastCheck" />

                    <TextView android:ellipsize="end"
                        android:gravity="start"
                        android:id="@+id/txtPrice"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/_8pxv"
                        android:layout_width="match_parent"
                        android:singleLine="true"
                        android:text="@string/lbl_185_00"
                        style="@style/txtOverpasssemibold16"/>
                </LinearLayout>
            </LinearLayout>

            <TextView
                android:ellipsize="end"
                android:id="@+id/txtDeliveryAddres"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_5pxh"
                android:layout_marginTop="@dimen/_21pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/msg_delivery_addres"
                style="@style/txtOverpasssemibold16"/>

<!--            <androidx.recyclerview.widget.RecyclerView-->
<!--                android:gravity="center_horizontal"-->
<!--                android:id="@+id/recyclerdeliveryaddress"-->
<!--                android:layout_height="wrap_content"-->
<!--                android:layout_marginEnd="@dimen/_3pxh"-->
<!--                android:layout_marginStart="@dimen/_3pxh"-->
<!--                android:layout_marginTop="@dimen/_18pxv"-->
<!--                android:layout_width="match_parent"-->
<!--                android:orientation="vertical"-->
<!--                android:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"-->
<!--                tools:itemCount="2"/>-->

            <LinearLayout
                android:gravity="end"
                android:id="@+id/linearRowgroup3639"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_3pxh"
                android:layout_marginStart="@dimen/_223pxh"
                android:layout_marginTop="@dimen/_7pxv"
                android:layout_width="wrap_content"
                android:orientation="horizontal">

                <ImageView
                    android:id="@+id/imageGroup3639"
                    android:layout_height="@dimen/_12pxh"
                    android:layout_marginTop="@dimen/_3pxv"
                    android:layout_width="@dimen/_12pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/plus_icon"
                    tools:ignore="ContentDescription"/>

                <TextView android:ellipsize="end"
                    android:id="@+id/txtAddAddress"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_weight="0.87"
                    android:layout_width="0sp"
                    android:lineSpacingExtra="@dimen/_1pxh"
                    android:singleLine="true"
                    android:textSize="16sp"
                    android:text="@string/lbl_add_address"
                    style="@style/txtOverpassromanregular14_1"/>
            </LinearLayout>

            <TextView
                android:ellipsize="end"
                android:id="@+id/txtPaymentmethod"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_10pxh"
                android:layout_marginStart="@dimen/_3pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_payment_method"
                style="@style/txtOverpasssemibold16"/>

            <LinearLayout
                android:id="@+id/linearRowellipsethirtyone"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:orientation="horizontal"
                style="@style/groupStylewhite_4">

                <LinearLayout
                    android:id="@+id/linearRowcashondeliver"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_7pxv"
                    android:layout_marginStart="@dimen/_15pxh"
                    android:layout_marginTop="@dimen/_9pxv"
                    android:layout_weight="0.92"
                    android:layout_width="0dp"
                    android:orientation="horizontal">

                    <FrameLayout
                        android:id="@+id/frameStack"
                        android:layout_height="@dimen/_38pxv"
                        android:layout_width="@dimen/_41pxh"
                        style="@style/groupStylecornerRadius_2">

                        <ImageView
                            android:id="@+id/imageImage140"
                            android:layout_gravity="top|center"
                            android:layout_height="@dimen/_23pxh"
                            android:layout_marginBottom="@dimen/_10pxv"
                            android:layout_marginEnd="@dimen/_3pxh"
                            android:layout_marginStart="@dimen/_4pxh"
                            android:layout_marginTop="@dimen/_6pxv"
                            android:layout_width="@dimen/_34pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/paypal_img"
                            tools:ignore="ContentDescription"/>
                    </FrameLayout>

                    <TextView
                        android:ellipsize="end"
                        android:id="@+id/txtCashonDeliver"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_9pxv"
                        android:layout_marginStart="@dimen/_19pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_weight="0.72"
                        android:layout_width="0dp"
                        android:textSize="16sp"
                        android:singleLine="true"
                        android:text="@string/msg_cash_on_deliver"
                        style="@style/txtOverpasssemibold14"
                        tools:ignore="NestedWeights" />
                </LinearLayout>

                <View
                    android:background="@drawable/rectangle_border_bluegray_900_3f_radius_7"
                    android:id="@+id/viewEllipseThirtyOne"
                    android:layout_height="@dimen/_14pxh"
                    android:layout_marginBottom="@dimen/_19pxv"
                    android:layout_marginEnd="@dimen/_24pxh"
                    android:layout_marginStart="@dimen/_102pxh"
                    android:layout_marginTop="@dimen/_22pxv"
                    android:layout_width="@dimen/_14pxh"/>
            </LinearLayout>

            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end"
                android:gravity="center"
                android:id="@+id/btnPayNow185Zero"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_38pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_17pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_17pxv"
                android:singleLine="true"
                android:text="@string/lbl_pay_now_185_00"
                android:textAllCaps="false"
                style="@style/btnSolidRounded"/>
        </LinearLayout>
    </FrameLayout>
</layout>
