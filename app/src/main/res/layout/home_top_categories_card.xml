<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="@dimen/_70sp"
    android:layout_height="@dimen/_120pxh"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical">


    <FrameLayout
        android:id="@+id/frameStack"
        style="@style/homtopcategoriesbg"
        android:layout_marginStart="5sp"
        android:layout_marginTop="@dimen/_5pxh"
        android:layout_width="@dimen/_65sp"
        android:layout_height="@dimen/_75pxv">

        <ImageView
            android:id="@+id/imageCardItems"
            android:layout_width="@dimen/_60pxh"
            android:layout_height="@dimen/_65pxh"
            android:layout_gravity="center_horizontal"/>
    </FrameLayout>

    <TextView
        android:id="@+id/txtTopCategoriesName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_10pxv"
        android:layout_gravity="center"
        android:textSize="@dimen/_16pxh"
        android:lineSpacingExtra="@dimen/_6pxh"
        android:text="Dental"
        android:textColor="@color/black" />
</LinearLayout>
